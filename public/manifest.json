{
  "manifest_version": 3,
  "name": "Bookmark"
,  "version": "1.0.0",
  "description": "Gets the current URL and displays it in a new tab.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}